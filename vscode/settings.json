{
  "workbench.startupEditor": "newUntitledFile",
  "explorer.confirmDragAndDrop": false,
  "headwind.runOnSave": true,
  "headwind.classRegex": {
    "html": "\\bclass\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\']",
    "css": "\\B@apply\\s+([_a-zA-Z0-9\\s\\-\\:\\/]+);",
    "javascript": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
    "javascriptreact": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
    "typescript": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)",
    "typescriptreact": "(?:\\bclassName\\s*=\\s*[\\\"\\']([_a-zA-Z0-9\\s\\-\\:\\/]+)[\\\"\\'])|(?:\\btw\\s*`([_a-zA-Z0-9\\s\\-\\:\\/]*)`)"
  },
  "[vue]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.tabSize": 2,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 4,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[python]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.python"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "eslint.codeAction.showDocumentation": {
    "enable": true
  },
  "aws.profile": "profile:default",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  // Personal setup
  "eslint.run": "onSave",
  "eslint.format.enable": true,
  "eslint.runtime": "",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "eslint.alwaysShowStatus": true,
  "files.autoSave": "onFocusChange",
  "window.autoDetectColorScheme": true,
  "terminal.integrated.tabs.enabled": true,
  "editor.formatOnPaste": true,
  "editor.detectIndentation": false,
  "editor.tabCompletion": "on",
  "editor.stickyTabStops": true,
  "tabnine.experimentalAutoImports": true,
  "cSpell.userWords": [
    "autogenerate",
    "bcrypt",
    "boto",
    "chdir",
    "Cmds",
    "connexion",
    "Firestore",
    "instabot",
    "popd",
    "pushd",
    "pydantic",
    "rprater",
    "setenv",
    "venv",
    "VPOPS",
    "Wireframe"
  ],
  "security.workspace.trust.untrustedFiles": "open",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "editor.minimap.enabled": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "python.formatting.provider": "black",
  "json.format.enable": true,
  "json.schemas": [],
  "tailwindCSS.includeLanguages": { "plaintext": "javascript" },
  "tailwindCSS.experimental.classRegex": [
    "tw`([^`]*)",
    ["classnames\\(([^)]*)\\)", "'([^']*)'"]
  ],
  "files.associations": {
    "*.py": "python"
  },
  "workbench.editor.untitled.hint": "hidden",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "editor.formatOnSave": true,
  "hediet.vscode-drawio.codeLinkActivated": true,
  "editor.suggest.showMethods": true,
  "editor.suggest.preview": true,
  "editor.acceptSuggestionOnEnter": "on",
  "editor.snippetSuggestions": "top",
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": false,
    "markdown": false,
    "javascript": false
  },
  "window.zoomLevel": -1
  //   "[typescript]": {
  //     "editor.defaultFormatter": "vscode.typescript-language-features"
  //   }
}
